<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<location path="." inheritInChildApplications="false">
		<system.webServer>
			<handlers>
				<add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
			</handlers>
			
			<aspNetCore processPath=".\wsPLD 8.exe" stdoutLogEnabled="true" stdoutLogFile=".\logs\stdout" hostingModel="inprocess" />

			<security>
				<requestFiltering>
					<requestLimits maxAllowedContentLength="209715200" />
				</requestFiltering>
			</security>
		</system.webServer>
	</location>
</configuration>